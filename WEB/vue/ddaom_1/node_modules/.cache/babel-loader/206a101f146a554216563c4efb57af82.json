{"ast":null,"code":"export default {\n  conponents: {},\n\n  data() {\n    return {\n      sampleData: ''\n    };\n  },\n\n  setup() {},\n\n  created() {},\n\n  mounted() {},\n\n  methods: {\n    divcheck() {\n      const che = document.querySelector('li input');\n      che.checked = true;\n    },\n\n    addList() {\n      const textarea = document.getElementById('textarea').value;\n      const li = document.createElement('li');\n      li.style.fontSize = '20px';\n      li.style.lineHeight = '50px';\n      const check = document.createElement('input');\n      check.setAttribute('type', 'checkbox');\n      check.setAttribute('class', 'ch');\n      const editbutton = document.createElement('button');\n      editbutton.innerText = '수정';\n      editbutton.style.width = '50px';\n      editbutton.style.height = '35px';\n      editbutton.style.fontSize = '16px';\n      editbutton.style.fontWeight = 'bold';\n      editbutton.style.marginTop = '8px';\n      editbutton.addEventListener('click', edittext);\n      const savebutton = document.createElement('button');\n      savebutton.innerText = '저장';\n      savebutton.style.float = 'right';\n      savebutton.style.width = '50px';\n      savebutton.style.height = '35px';\n      savebutton.style.fontSize = '16px';\n      savebutton.style.fontWeight = 'bold';\n      savebutton.style.marginLeft = '8px';\n      savebutton.style.marginTop = '8px'; // savebutton.addEventListener(\"click\",savetext);\n\n      editbutton.style.float = 'right';\n      li.style.height = '50px';\n      const textNode = document.createTextNode(textarea);\n      li.appendChild(check);\n      li.appendChild(textNode);\n      li.appendChild(savebutton);\n      li.appendChild(editbutton);\n      document.getElementById('todo').appendChild(li);\n      document.getElementById('textarea').value = '';\n      document.getElementById('textarea').focus();\n\n      function edittext() {\n        textNode.remove();\n        const edittextbox = document.createElement('input');\n        edittextbox.type = 'text';\n        edittextbox.setAttribute('id', 'edit');\n        this.parentElement.appendChild(edittextbox);\n      }\n    },\n\n    removeItem() {\n      // const ul = document.getElementById('todo')\n      const chb = document.querySelectorAll('input[type=checkbox]');\n\n      for (var i = 0; i < chb.length; i++) {\n        if (chb[i].checked === true) {\n          chb[i].parentElement.remove();\n        }\n      }\n    },\n\n    selectAll() {\n      const chb = document.querySelectorAll('.ch');\n      chb.forEach(checkbox => {\n        checkbox.checked = true;\n      });\n    },\n\n    unSelectAll() {\n      const chb = document.querySelectorAll('.ch');\n      chb.forEach(checkbox => {\n        checkbox.checked = false;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,eAAe;EACbA,UAAU,EAAE,EADC;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;IADP,CAAP;EAGD,CANY;;EAObC,KAAK,GAAG,CAAE,CAPG;;EAQbC,OAAO,GAAG,CAAE,CARC;;EASbC,OAAO,GAAG,CAAE,CATC;;EAUbC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,MAAMC,GAAE,GAAIC,QAAQ,CAACC,aAAT,CAAuB,UAAvB,CAAZ;MACAF,GAAG,CAACG,OAAJ,GAAc,IAAd;IACD,CAJM;;IAKPC,OAAO,GAAG;MACR,MAAMC,QAAO,GAAIJ,QAAQ,CAACK,cAAT,CAAwB,UAAxB,EAAoCC,KAArD;MACA,MAAMC,EAAC,GAAIP,QAAQ,CAACQ,aAAT,CAAuB,IAAvB,CAAX;MAEAD,EAAE,CAACE,KAAH,CAASC,QAAT,GAAoB,MAApB;MACAH,EAAE,CAACE,KAAH,CAASE,UAAT,GAAsB,MAAtB;MAEA,MAAMC,KAAI,GAAIZ,QAAQ,CAACQ,aAAT,CAAuB,OAAvB,CAAd;MACAI,KAAK,CAACC,YAAN,CAAmB,MAAnB,EAA2B,UAA3B;MACAD,KAAK,CAACC,YAAN,CAAmB,OAAnB,EAA4B,IAA5B;MAEA,MAAMC,UAAS,GAAId,QAAQ,CAACQ,aAAT,CAAuB,QAAvB,CAAnB;MACAM,UAAU,CAACC,SAAX,GAAuB,IAAvB;MACAD,UAAU,CAACL,KAAX,CAAiBO,KAAjB,GAAyB,MAAzB;MACAF,UAAU,CAACL,KAAX,CAAiBQ,MAAjB,GAA0B,MAA1B;MACAH,UAAU,CAACL,KAAX,CAAiBC,QAAjB,GAA4B,MAA5B;MACAI,UAAU,CAACL,KAAX,CAAiBS,UAAjB,GAA8B,MAA9B;MACAJ,UAAU,CAACL,KAAX,CAAiBU,SAAjB,GAA6B,KAA7B;MAEAL,UAAU,CAACM,gBAAX,CAA4B,OAA5B,EAAqCC,QAArC;MAEA,MAAMC,UAAS,GAAItB,QAAQ,CAACQ,aAAT,CAAuB,QAAvB,CAAnB;MACAc,UAAU,CAACP,SAAX,GAAuB,IAAvB;MACAO,UAAU,CAACb,KAAX,CAAi